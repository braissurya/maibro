	<queryString>
		<![CDATA[SELECT  p.id,p.posisi posisipolis,
		p.spaj_no, p.policy_no, p.asuransi_id, plafon_kredit, p.spaj_date, 
		p.ins_period, p.beg_date, p.end_date, p.jenis_manfaat, p.pay_mode,  
		p.gol_debitur, p.kode_cabang, p.username, p.dealref, p.jenis_manfaat,
		c.tempat_lahir, c.tgl_lahir, c.umur,p.createdate,
		c.nama namacustomer, c.gender, c.berat_badan, c.tinggi_badan, c.pekerjaan,
		c.bagian, c.q1,c.q1_desc,c.q2,c.q2_desc,c.q3,c.q3_desc,c.q4,c.q4_desc,c.q5,c.q5_desc,
		mb.polis_induk, b.clause,
		pr.premi, pr.up, pr.rate, pr.tgl_paid, pr.nominal_paid,
		mp.kode, mp.nama namaproduk, mp.rek_no, mp.rek_nama,
		mb.nama namaasuransi, mcb.nama namacabang, mb2.nama namabank, 
		a.alamat_rumah,  a.kota_rumah, a.kodepos_rumah, a.alamat_kantor, a.kota_kantor, a.kodepos_kantor,
		a.telp_rumah, a.telp_kantor,a.telp_hp, pr.biaya_admin,
		bf.nama nama_benef, bf.relasi_benef, bf.relasi,
		ag.nama nama_agen, ag.telp_hp telp_agen,ag.jabatan jabatan_agen
		FROM policy p left join mst_bank mb   on p.asuransi_id = mb.id
			left join (select bnf.*, if(bnf.relasi_lain is not null,mms.keterangan,bnf.relasi_lain) relasi_benef from beneficiary bnf, mst_master mms where bnf.relasi = mms.jenis and mms.id=2) bf on p.id = bf.policy_id  
			left join agent ag on p.id = ag.policy_id,
			customer c, product pr, 
			 mst_product mp, group_policy gp, 
			 bank b, mst_cab_bank mcb, mst_bank mb2,
			 address a
		where gp.id = p.group_policy_id
		and p.customer_id = c.id
		and pr.policy_id = p.id
		and mp.id= pr.mst_product_id
		and b.policy_id = p.id
		and c.address_id = a.id
		and b.mst_cab_bank_id = mcb.id
		and mcb.mst_bank_id = mb2.id
		and p.id=$P{id}]]>
	</queryString>
