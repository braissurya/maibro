	<queryString>
		<![CDATA[SELECT  p.id,p.posisi posisipolis,
				p.spaj_no, p.policy_no, p.asuransi_id, plafon_kredit, p.spaj_date, 
				p.ins_period, p.beg_date, p.end_date, p.jenis_manfaat, p.pay_mode,  
				p.gol_debitur, p.kode_cabang, p.username, p.dealref,
				c.nama namacustomer, mb.polis_induk, b.clause,
				pr.premi, pr.up, pr.rate, pr.tgl_paid, pr.nominal_paid,
				mp.kode, mp.nama namaproduk, mp.rek_no, mp.rek_nama,	
				mb.nama namaasuransi, mcb.nama namacabang, mb2.nama namabank, 
				a.alamat_rumah,  IFNULL(a.kota_rumah, '') kota_rumah, IFNULL(a.kodepos_rumah, '') kodepos_rumah,
				o.alamat_rumah alamat_objek, IFNULL(o.kota_rumah, '') kota_objek, IFNULL(o.kodepos_rumah, '') kode_objek,
				a.telp_rumah, a.telp_kantor,a.telp_hp, pr.biaya_admin, o.no_sertifikat, o.type_sertifikat,
                (case when o.jenis_bangunan = 3 then o.penggunaan_bangunan else (select keterangan from mst_master where id = 5 and jenis = o.jenis_bangunan) end) gunbang,
                o.harga_bangunan, o.harga_stock, o.harga_perabot, o.harga_lainnya, o.luas_bangunan, o.luas_tanah,
                o.kondisi_kiri, o.kondisi_kanan, o.kondisi_depan, o.kondisi_belakang, ag.nama agent_nama, ag.telp_hp agent_telp, ag.jabatan agent_jabatan
		FROM policy p left join mst_bank mb on p.asuransi_id = mb.id ,customer c, product pr, 
			 mst_product mp, group_policy gp, 
			 bank b, mst_cab_bank mcb, mst_bank mb2,
			 address a, address o, agent ag
		where gp.id = p.group_policy_id
		and p.customer_id = c.id
		and pr.policy_id = p.id
		and mp.id= pr.mst_product_id
		and b.policy_id = p.id
		and c.address_id = a.id
		and c.objek_pertanggungan = o.id
		and b.mst_cab_bank_id = mcb.id
		and mcb.mst_bank_id = mb2.id
        and p.id = ag.policy_id
		and p.id=$P{id}]]>
	</queryString>
