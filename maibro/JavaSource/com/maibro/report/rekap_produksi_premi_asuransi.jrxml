	<queryString>
		<![CDATA[SELECT ass.jenis_produk, ass.ass_name, ass.diskon_premi, ass.diskon_brokerage, ass.diskon_komisi, ass.ppn_premi, ass.pph_premi, ass.pph_bpr, ass.pph_agent, 
	   COALESCE(SUM(jum_polis), 0) jum_polis,
	   COALESCE(SUM(up), 0) up, COALESCE(SUM(premi), 0) premi, 
	   COALESCE(SUM(gross_bkg), 0) gross_bkg, COALESCE(SUM(premi) - SUM(gross_bkg), 0) premi_netto,
	   COALESCE(SUM(gross_bkg * (ass.ppn_premi / 100)), 0) ppn10, COALESCE(SUM(gross_bkg * (ass.pph_premi / 100)), 0) pph2
FROM (
	SELECT pro.nama jenis_produk, ass.nama ass_name, p.asuransi_id, 1 jum_polis, pr.up, pr.premi, 
		   ROUND((pr.premi * ass.diskon_premi) / 100, 2) gross_bkg
	FROM policy p, bank b, mst_cab_bank cb, mst_bank ba, mst_bank ass, 
		 product pr, mst_product pro
	WHERE p.id = b.policy_id
	AND b.mst_cab_bank_id = cb.id
	AND cb.mst_bank_id = ba.id
	AND p.asuransi_id = ass.id
	AND p.id = pr.policy_id
	AND pr.mst_product_id = pro.id
	AND $P!{param}
) x, ( SELECT p.nama as jenis_produk, b.nama ass_name, b.id, b.diskon_premi, b.diskon_brokerage, b.diskon_komisi, b.ppn_premi, b.pph_premi, b.pph_bpr, b.pph_agent
				 FROM mst_bank b, mst_product p
				 WHERE b.jenis IN (2,4)
			     $P!{param2}
			   ) ass where x.asuransi_id = ass.id AND x.jenis_produk = ass.jenis_produk
GROUP BY ass.jenis_produk, ass.ass_name, ass.diskon_premi, ass.diskon_brokerage, ass.diskon_komisi, ass.ppn_premi, ass.pph_premi, ass.pph_bpr, ass.pph_agent
;]]>
	</queryString>
